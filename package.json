{
  "name": "fast-react",
  "version": "0.0.1",
  "description": "A CLI tool for creating lightweight React apps",
  "main": "build/lib/cli.js",
  "scripts": {
    "test": "jest --coverage",
    "ci:lint": "eslint . -c .eslintrc.json",
    "start": "tsc && node ./dist/lib/cli.js",
