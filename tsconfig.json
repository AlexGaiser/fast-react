{
  "compilerOptions": {
    "target": "es5",
    "module": "ES6",
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": false,
    "declaration": true,
    "composite": true,
    "moduleResolution": "node",
    "outDir":"build",
  },
  "include": ["src/**/*"],
  "exclude": ["*.test.ts"]
}
